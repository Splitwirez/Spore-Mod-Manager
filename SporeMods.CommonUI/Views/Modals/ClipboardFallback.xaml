<UserControl x:Class="SporeMods.CommonUI.ClipboardFallback"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:local="clr-namespace:SporeMods.CommonUI"
			 mc:Ignorable="d" 
			 d:DesignHeight="450" d:DesignWidth="800">
	<DockPanel>
		<TextBlock x:Name="InstructionTextBlock" Margin="25,25,25,12" DockPanel.Dock="Top"/>
		<TextBox x:Name="ContentTextBox" Padding="25,12,25,25" IsReadOnly="True"/>
	</DockPanel>
</UserControl>
